{
	"dataPath": "/home/cwilkin/Development/repos/marketinsights-pipeline/datasets/",
	"name": "dataclean",
	"description": "Convert RAW data to clean OHLC",
	"datasources": [
		{
			"name": "tradefair",
			"source": "https://www.tradefair.com",
			"date_format": "dd-MM-yy",
			"time_format": "HH:mm:ss",
			"index_col": 0,
			"parse_dates": [[0,1]],
			"dayfirst": true,
			"skiprows": 1,
			"timezone":"Europe/London",
			"timeseries": [
				{
					"name":"WallSt-hourly",
					"market": "DOW",															
					"timezone":"US/Eastern"
				},
				{
					"name":"SP500-hourly",
					"market": "SPY",															
					"timezone":"US/Eastern"
				}
			]

		},
		{
			"name": "finam",
			"source": "https://www.finam.ru/profile/mirovye-indeksy/d-j-ind/export",
			"date_format": "dd-MM-yy",
			"time_format": "HH:mm:ss",
			"index_col": 0,
			"parse_dates": [[0,1]],
			"dayfirst": true,
			"skiprows": 0,
			"timezone":"US/Eastern",
			"timeseries": [
				{
					"name":"D&J-IND",	
					"market": "DOW",				
					"timezone":"US/Eastern"
				},
				{
					"name":"SANDP-500",					
					"market": "SPY",
					"timezone":"US/Eastern"
				}
			]

		}
	]
}
